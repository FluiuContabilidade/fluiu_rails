<%= form_for @user, url: 'edit', html: { method: :post } do |f| %>
  <div class="" style="width:60%; margin:0 auto;">
    <div class="form-group">
      <%= f.label :das_file, 'DAS'%>
      <%= f.file_field :das_file%>
    </div>
    <div class="form-group">
      <%= f.label :fgts, 'FGTS'%>
      <%= f.file_field :fgts%>
    </div>
    <div class="form-group">
      <%= f.label :inss, 'INSS'%>
      <%= f.file_field :inss%>
    </div>
    <div class="form-group">
      <%= f.label :fau, 'FAU'%>
      <%= f.file_field :fau%>
    </div>
    <div class="form-group">
      <%= f.label :tributary_sub, 'Substituição Tributária'%>
      <%= f.file_field :tributary_sub%>
    </div>
    <div class="form-group">
      <%= f.label :payment_installments, 'Parcelamentos'%>
      <%= f.file_field :payment_installments%>
    </div>
    <%= f.submit "Enviar", class:'btn btn-default' %>
  </div>
<% end %>
